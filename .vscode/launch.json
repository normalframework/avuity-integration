{
  "version": "0.2.0",
  "configurations": [
    {
      "command": "npm start",
      "name": "Debug Dev Server",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "npx tsc && npx normal-application-run ${input:hookFile} ${input:configFile}",
      "name": "Debug Script",
      "request": "launch",
      "type": "node-terminal",
      "autoAttachChildProcesses": true
    }
  ],
  "inputs": [
    {
      "type": "promptString",
      "id": "hookFile",
      "description": "Choose Hook File"
    },
    {
      "type": "promptString",
      "id": "configFile",
      "description": "Choose Config File"
    }
  ]
}
